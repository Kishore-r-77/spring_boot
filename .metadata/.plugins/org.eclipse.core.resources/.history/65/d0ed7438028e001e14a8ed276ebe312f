package practice;

class Person{
	String name="Kishore";
	void display() {
		System.out.println("Hey this is "+this.name);
	}
	@Override
	public String toString() {
		return "Person [name=" + name + "] is the Best.";
	}
	
}

class Practice {

	public static void main(String[] args) {
		
		Person kishore = new Person();
		System.out.println(kishore);
	}
}
